<a href="/{{item.id}}">
    <div class="card item-card" style="width: 18rem;">
        <li class="list-group-item title">{{item.title}}</li>
        <img src="/images/tomato.jpg" class="card-img-top" alt="produce-pic">
        <div class="card-body">
            <h5 class="card-title">Quantity: ${{item.item_price}}/{{item.item_unit}}</h5>
            <p class="card-text">{{item.item_description}}</p>
        </div>
    </div>
</a>
